# Autofy LangChain Backend Environment Configuration

# Application Settings
APP_NAME="Autofy LangChain Backend"
DEBUG=true
SECRET_KEY=your-super-secret-key-change-this-in-production

# API Configuration
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000

# Database Configuration (use your existing Supabase config)
DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-role-key
SUPABASE_ANON_KEY=your-supabase-anon-key

# LLM Providers (at least one required)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Google OAuth (for Gmail integration)
GOOGLE_CLIENT_ID=your-google-oauth-client-id
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# Service Integrations
NOTION_API_KEY=your-notion-integration-token
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# LangSmith (Optional - for tracing and debugging)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your-langsmith-api-key
LANGCHAIN_PROJECT=autofy-workflows

# Allowed CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Performance Settings
MAX_RETRIES=3
RETRY_DELAY=1000
CONNECTION_POOL_SIZE=10
