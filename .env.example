# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend API Configuration (New LangChain Backend)
VITE_BACKEND_URL=http://localhost:8000

# OAuth Configuration (if using direct OAuth)
VITE_GMAIL_CLIENT_ID=your_gmail_client_id
VITE_NOTION_CLIENT_ID=your_notion_client_id

# Development Settings
VITE_DEBUG=true

# Legacy Backend Environment Variables (for Supabase Edge Functions)
# These should be set in your Supabase dashboard under Settings > Environment Variables
# Note: These are only needed if you're still using the old system

# OpenRouter API Key for AI Processing
# Get your free API key from: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Notion Integration (optional - for fallback when OAuth isn't available)
NOTION_INTERNAL_TOKEN=your_notion_internal_integration_token

# Gmail OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
